{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Chat_Sidebar.js", "components/Loading.js", "components/Feed/Post.tsx", "pages/users/[User].tsxx", "components/Chat/ChatSidebar.js", "components/Chat/Chat.js", "pages/users/[User].tsx"],
  "exclude": ["node_modules"]
}
